{
	"name": "@alanscodelog/eslint-config",
	"version": "5.0.4",
	"description": "My eslint config.",
	"main": "./config.js",
	"type": "module",
	"exports": {
		".": "./config.js"
	},
	"scripts": {
		"print": "eslint --print-config .eslintrc.js",
		"lint:eslint": "eslint \"{tests,copy,rules}/**/*.{cjs,js,ts}\" \"*.{cjs,js,ts}\" --max-warnings=1 --report-unused-disable-directives",
		"lint:eslint:test": "eslint \"{src}/**/*.{cjs,js,ts}\"",
		"lint:commits": "commitlint --from $(git rev-list --max-parents=0 HEAD) --to HEAD --verbose",
		"test": "node tests/loads.js",
		"lint": "pnpm lint:eslint",
		"postinstall": "node copy/postinstall.js",
		"prepare": "husky"
	},
	"dependencies": {
		"@eslint/js": "^9.12.0",
		"@stylistic/eslint-plugin": "^2.9.0",
		"@typescript-eslint/eslint-plugin": "^8.8.1",
		"@typescript-eslint/parser": "^8.8.1",
		"@typescript-eslint/utils": "^8.8.1",
		"@vue/eslint-config-typescript": "^14.0.0",
		"eslint": "=9.12.0",
		"eslint-import-resolver-typescript": "^3.6.3",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-jsdoc": "^48.0.0",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"eslint-plugin-vue": "^9.28.0",
		"globals": "^15.10.0",
		"typescript-eslint": "^8.8.1",
		"vue-eslint-parser": "^9.4.3"
	},
	"devDependencies": {
		"@alanscodelog/commitlint-config": "^3.0.2",
		"@alanscodelog/semantic-release-config": "^5.0.4",
		"@alanscodelog/tsconfigs": "^6.0.0",
		"@commitlint/cli": "^19.5.0",
		"@types/eslint": "^9.6.1",
		"@types/node": "^22.7.5",
		"husky": "^9.1.6",
		"semantic-release": "^24.1.2",
		"typescript": "~5.5.0"
	},
	"author": "Alan <alanscodelog@gmail.com>",
	"repository": "https://github.com/alanscodelog/eslint-config",
	"license": "MIT",
	"files": [
		"config.js",
		"rules",
		"install.sh",
		"copy"
	],
	"eslintIgnore": [],
	"release": {
		"extends": [
			"@alanscodelog/semantic-release-config"
		]
	},
	"commitlint": {
		"extends": [
			"@alanscodelog"
		]
	},
	"engines": {
		"node": ">=20.0.0"
	},
	"@comments": {
		"devDependencies": {
			"@alanscodelog/tsconfigs": "For local testing against my typescript config."
		},
		"scripts": {
			"lint:commits": "Lints all unpushed commits in the active branch."
		}
	},
	"TEMPLATE:4df5c37": "Ignore. Commit of the template this was created with / last updated to.",
	"publishConfig": {
		"access": "public"
	}
}
