{
	"name": "@alanscodelog/eslint-config",
	"version": "0.0.0-semantically-released",
	"description": "My eslint config.",
	"main": ".eslintrc.js",
	"scripts": {
		"print": "eslint --print-config .eslintrc.js",
		"show-unset": "node tests/no_unset.js",
		"lint:eslint": "eslint \"{src,tests,copy}/**/*.{js,ts}\" \"*.{js,ts}\" --max-warnings=0 --report-unused-disable-directives",
		"lint:commits": "commitlint --from $(git rev-list HEAD --not --remotes | tail -1)^ --to HEAD --verbose",
		"test": "node tests/loads.js && npm run show-unset",
		"postinstall": "node copy/postinstall.js",
		"prepare": "husky install"
	},
	"peerDependencies": {
		"typescript": ">=4.4.4",
		"@typescript-eslint/eslint-plugin": ">=5.3.1",
		"@typescript-eslint/parser": ">=5.3.1",
		"eslint": ">=7.23.0",
		"eslint-import-resolver-typescript": ">=2.5.0",
		"eslint-plugin-import": ">=2.25.3",
		"eslint-plugin-simple-import-sort": ">=7.0.0",
		"eslint-plugin-jsdoc": ">=37.0.3",
		"eslint-plugin-vue": ">=8.0.3",
		"@vue/eslint-config-typescript": ">=9.0.1"
	},
	"devDependencies": {
		"@types/node": "^16.11.7",
		"@types/eslint": "^7.28.2",

		"typescript": ">=4.4.4",
		"@typescript-eslint/eslint-plugin": ">=5.3.1",
		"@typescript-eslint/parser": ">=5.3.1",
		"eslint": "^8.2.0",
		"eslint-import-resolver-typescript": ">=2.5.0",
		"eslint-plugin-import": ">=2.25.3",
		"eslint-plugin-simple-import-sort": ">=7.0.0",
		"eslint-plugin-jsdoc": ">=37.0.3",
		"eslint-plugin-vue": ">=8.0.3",
		"@vue/eslint-config-typescript": ">=9.0.1",

		"@alanscodelog/tsconfigs": "^3.0.0",

		"eslint-find-rules": "^4.0.0",

		"semantic-release": "^18.0.0",
		"@semantic-release/changelog": "^6.0.1",
		"@semantic-release/commit-analyzer": "^9.0.1",
		"@semantic-release/github": "^8.0.2",
		"@semantic-release/npm": "^8.0.3",
		"@semantic-release/release-notes-generator": "^10.0.2",
		"conventional-changelog-conventionalcommits": "^4.6.1",
		"@alanscodelog/semantic-release-config": "^2.0.0",
		"@alanscodelog/commitlint-config": "^2.0.0",
		"commitlint": "^15.0.0",
		"husky": "^7.0.4"
	},
	"author": "Alan <alanscodelog@gmail.com>",
	"repository": "https://github.com/alanscodelog/my-eslint-config",
	"license": "MIT",
	"files": [
		".eslintrc.js",
		"base.js",
		"js.js",
		"typescript.js",
		"vue.js",
		"tests.js",
		"install.sh",
		"copy"
	],
	"eslintIgnore": [],
	"release": { "extends": [ "@alanscodelog/semantic-release-config" ] },
	"commitlint": { "extends": [ "@alanscodelog" ] },
	"engines": { "node": ">=14.0.0" },
	"@comments": {
		"devDependencies": {
			"@alanscodelog/tsconfigs": "For local testing against my typescript config."
		},
		"scripts": {
			"lint:commits": "Lints all unpushed commits in the active branch."
		}
	},
	"TEMPLATE:4df5c37": "Ignore. Commit of the template this was created with / last updated to.",
	"publishConfig": {
		"access": "public"
	}
}
