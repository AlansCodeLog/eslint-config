{
	"name": "@alanscodelog/eslint-config",
	"version": "0.0.0-semantically-released",
	"description": "My eslint config.",
	"main": ".eslintrc.js",
	"scripts": {
		"print": "npx eslint --print-config .eslintrc.js",
		"print:missing": "npm run print | grep -A10000 \"wrap-regex\"",
		"lint:test": "npx eslint \"src/**/*\"",
		"semantic-release": "semantic-release"
	},
	"author": "Alan <alanscodelog@gmail.com>",
	"license": "MIT",
	"peerDependencies": {
		"typescript": ">=3.7",
		"@typescript-eslint/eslint-plugin": ">= 3.0.0",
		"@typescript-eslint/parser": ">= 3.0.0",
		"eslint": ">= 7.0.0",
		"eslint-import-resolver-typescript": ">= 2.0.0",
		"eslint-plugin-import": ">= 2.20.0",
		"eslint-plugin-simple-import-sort": ">= 5.0.0"
	},
	"devDependencies": {
		"semantic-release": "^17.0.7",
		"@semantic-release/changelog": "^5.0.1",
		"@semantic-release/commit-analyzer": "^8.0.1",
		"@semantic-release/github": "^7.0.5",
		"@semantic-release/npm": "^7.0.5",
		"@semantic-release/release-notes-generator": "^9.0.1",
		"conventional-changelog-conventionalcommits": "^4.3.0",

		"typescript": ">=3.7",
		"@typescript-eslint/eslint-plugin": ">= 3.0.0",
		"@typescript-eslint/parser": ">= 3.0.0",
		"eslint": ">= 7.0.0",
		"eslint-import-resolver-typescript": ">= 2.0.0",
		"eslint-plugin-import": ">= 2.20.0",
		"eslint-plugin-simple-import-sort": ">= 5.0.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/AlansCodeLog/my-eslint-config.git"
	},
	"bugs": {
		"url": "https://github.com/AlansCodeLog/my-eslint-config/issues"
	},
	"homepage": "https://github.com/AlansCodeLog/my-eslint-config#readme",
	"files": [
		".eslintrc.js",
		"tests.js",
		"install.sh",
		"copy/*"
	],
	"eslintIgnore": [
		"copy/*",
		"tests.js"
	],
	"publishConfig": {
		"access": "public"
	}
}
