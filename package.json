{
	"name": "@alanscodelog/eslint-config",
	"version": "0.0.0-semantically-released",
	"description": "My eslint config.",
	"main": ".eslintrc.js",
	"scripts": {
		"print": "eslint --print-config .eslintrc.js",
		"show-unset": "node tests/no_unset.js",
		"lint:eslint": "eslint \"{src,tests,copy}/**/*.{cjs,js,ts}\" \"*.{cjs,js,ts}\" --max-warnings=0 --report-unused-disable-directives",
		"lint:commits": "commitlint --from $(git rev-list HEAD --not --remotes | tail -1)^ --to HEAD --verbose",
		"test": "node tests/loads.js && npm run show-unset",
		"postinstall": "node copy/postinstall.js",
		"prepare": "husky install"
	},
	"peerDependencies": {
		"eslint": ">=8.19.0",
		"eslint-plugin-simple-import-sort": ">=7.0.0",
		"eslint-plugin-import": ">=2.26.0",
		"eslint-plugin-jsdoc": ">=39.3.3",

		"typescript": ">=4.7.4",
		"@typescript-eslint/parser": ">=5.30.5",
		"@typescript-eslint/eslint-plugin": ">=5.30.5",
		"eslint-import-resolver-typescript": ">=3.2.4",

		"eslint-plugin-vue": ">=9.2.0",
		"@vue/eslint-config-typescript": ">=11.0.0"
	},
	"peerDependenciesMeta": {
		"typescript": {"optional": true},
		"@typescript-eslint/parser": {"optional": true},
		"@typescript-eslint/eslint-plugin": {"optional": true},
		"eslint-import-resolver-typescript": {"optional": true},

		"eslint-plugin-vue": {"optional": true},
		"@vue/eslint-config-typescript": {"optional": true}
	},
	"devDependencies": {
		"@types/node": "^18.0.3",
		"@types/eslint": "^8.4.5",

		"eslint":"^8.19.0",
		"eslint-plugin-simple-import-sort": "^7.0.0",
		"eslint-plugin-import":"^2.26.0",
		"eslint-plugin-jsdoc": "^39.3.3",

		"typescript": "^4.7.4",
		"@typescript-eslint/parser": "^5.30.5",
		"@typescript-eslint/eslint-plugin": "^5.30.5",
		"eslint-import-resolver-typescript": "^3.2.4",

		"eslint-plugin-vue": "^9.2.0",
		"@vue/eslint-config-typescript": "^11.0.0",

		"@alanscodelog/tsconfigs": "^3.0.0",

		"eslint-find-rules": "^4.1.0",

		"semantic-release": "^19.0.3",
		"@semantic-release/changelog": "^6.0.1",
		"@semantic-release/commit-analyzer": "^9.0.2",
		"@semantic-release/github": "^8.0.4",
		"@semantic-release/npm": "^9.0.1",
		"@semantic-release/release-notes-generator": "^10.0.3",
		"conventional-changelog-conventionalcommits": "^5.0.0",
		"@alanscodelog/semantic-release-config": "^2.0.0",
		"@alanscodelog/commitlint-config": "^2.0.0",
		"commitlint": "^17.0.3",
		"husky": "^8.0.1"
	},
	"author": "Alan <alanscodelog@gmail.com>",
	"repository": "https://github.com/alanscodelog/my-eslint-config",
	"license": "MIT",
	"files": [
		".eslintrc.js",
		"base.js",
		"js.js",
		"typescript.js",
		"vue.js",
		"tests.js",
		"install.sh",
		"copy"
	],
	"eslintIgnore": [],
	"release": { "extends": [ "@alanscodelog/semantic-release-config" ] },
	"commitlint": { "extends": [ "@alanscodelog" ] },
	"engines": { "node": ">=14.0.0" },
	"@comments": {
		"devDependencies": {
			"@alanscodelog/tsconfigs": "For local testing against my typescript config."
		},
		"scripts": {
			"lint:commits": "Lints all unpushed commits in the active branch."
		}
	},
	"TEMPLATE:4df5c37": "Ignore. Commit of the template this was created with / last updated to.",
	"publishConfig": {
		"access": "public"
	}
}
